  <command:command xmlns:maml="http://schemas.microsoft.com/maml/2004/10" xmlns:command="http://schemas.microsoft.com/maml/dev/command/2004/10" xmlns:dev="http://schemas.microsoft.com/maml/dev/2004/10">
    <command:details>
      <command:name>
        Get-FaciliaPS_ServerSetup
      </command:name>
      <maml:description>
        <maml:para>
          Kommando, der viser et navn givent serversetup, hvis der ikke gives noget navn men bruges en "*" vises alle serversetups.
        </maml:para>
      </maml:description>
      <maml:copyright>
        <maml:para>Copyright © FACILIA FMBA</maml:para>
      </maml:copyright>
      <command:verb>Remove</command:verb>
      <dev:version>0.9/B</dev:version>
    </command:details>
    <command:syntax>
      <command:syntaxItem>
        <maml:name>Remove-FaciliaPS_ServerSetup</maml:name>
        <command:parameter required="true" position="0">
          <maml:name>ServerSetupNavn</maml:name>
          <maml:description>
            <maml:para>Det unikke navn på den serveropsætning, der skal vises. </maml:para>
          </maml:description>
          <command:parameterValue>string</command:parameterValue>
        </command:parameter>
      </command:syntaxItem>
    </command:syntax>
    <command:parameters>
      <command:parameter required="true" position="0">
        <maml:name>ServerSetupNavn</maml:name>
        <maml:description>
          <maml:para>Det unikke navn på den serveropsætning, der skal vises.</maml:para>
        </maml:description>
        <command:parameterValue>string</command:parameterValue>
      </command:parameter>
    </command:parameters>
    <command:examples>
      <command:example>
        <maml:title>

          ------------------------ Facilia server setup eksempel ------------------------

          Dette eksempel viser hvordan der vises et eller flere serversetups.
        </maml:title>
        <dev:code>
          $ServerSetupNavn  = "fac-webudv.facilia.dk"
          Get-FaciliaPS_ServerSetup -ServerSetupNavn $ServerSetupNavn
          eller
          $ServerSetupNavn  = "*"
          Get-FaciliaPS_ServerSetup -ServerSetupNavn $ServerSetupNavn
        </dev:code>
        <dev:remarks>
          <maml:para>Dette eksempel et tænkt som en mulig script function, der kan kaldes med samme parametre, hvor værdierne kan ændres.</maml:para>
        </dev:remarks>
      </command:example>
    </command:examples>
